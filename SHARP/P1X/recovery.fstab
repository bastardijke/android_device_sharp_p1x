# mount point   	fstype  	device					device2				flags
/boot			emmc		/dev/block/bootdevice/by-name/boot
/recovery		emmc		/dev/block/bootdevice/by-name/recovery		flags=display="Recovery";backup=1
/system_tmp		ext4		/dev/block/bootdevice/by-name/system		flags=backup=0;display="System"
/system_image		emmc		/dev/block/bootdevice/by-name/system
/data			f2fs		/dev/block/bootdevice/by-name/userdata		flags=encryptable=/dev/block/bootdevice/by-name/metadata
/data_image		emmc            /dev/block/bootdevice/by-name/userdata		flags=backup=0;encryptable=/dev/block/bootdevice/by-name/metadata;wipeduringfactoryreset
/cache			ext4		/dev/block/bootdevice/by-name/cache
/misc			emmc		/dev/block/bootdevice/by-name/misc
#/modem			vfat		/dev/block/bootdevice/by-name/modem			flags=backup=1;display="Modem (NON-HLOS)";flashimg=1
/firmware		emmc 		/dev/block/bootdevice/by-name/modem			flags=backup=1;display="Firmware";mounttodecrypt
/bluetooth		emmc		/dev/block/bootdevice/by-name/bluetooth		flags=backup=1;display="Bluetooth (BTFM)";flashimg=1
/persist		ext4		/dev/block/bootdevice/by-name/persist		flags=display="Persist";backup=1
/efs1			emmc		/dev/block/bootdevice/by-name/modemst1		flags=backup=1;display=EFS
/efs2			emmc		/dev/block/bootdevice/by-name/modemst2		flags=backup=1;subpartitionof=/efs1
/efsg           	emmc		/dev/block/bootdevice/by-name/fsg			flags=backup=1;subpartitionof=/efs1
/efsc           	emmc		/dev/block/bootdevice/by-name/fsc			flags=backup=1;subpartitionof=/efs1

/keystore       	emmc      	/dev/block/bootdevice/by-name/keystore
/keymaster		emmc	  	/dev/block/bootdevice/by-name/keymaster

# Removable storage
/external_sd		vfat		/dev/block/mmcblk1p1 	/dev/block/mmcblk1	flags=display="MicroSD";storage;wipeingui;removable
/usb-otg		vfat		/dev/block/sda1 		/dev/block/sda		flags=display="USB OTG";storage;wipeingui;removable
